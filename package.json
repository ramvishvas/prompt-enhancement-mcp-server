{
  "name": "prompt-enhancement-mcp-server",
  "version": "1.0.1",
  "description": "An MCP server that enhances user prompts using AI providers (Anthropic, OpenAI, OpenRouter, Gemini)",
  "main": "dist/index.js",
  "bin": {
    "prompt-enhancer": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ramvishvas/prompt-enhancement-mcp-server.git"
  },
  "bugs": {
    "url": "https://github.com/ramvishvas/prompt-enhancement-mcp-server/issues"
  },
  "homepage": "https://github.com/ramvishvas/prompt-enhancement-mcp-server#readme",
  "author": "ramvishvas",
  "scripts": {
